<h1>Work Readiness</h1>
{{#.}}
    <br>
    <table class="data-table">
        <tr>
            <td class="data-column-25">Created date</td>
            <td class="data-column-25">Modified date</td>
        </tr>
        <tr>
            <td>{{ optionalValue (formatDate createdDateTime)}}</td>
            <td>{{ optionalValue (formatDate modifiedDateTime)}}</td>
        </tr>
    </table>

    <h2>Profile data</h2>
    {{#profileData}}
        <table class="summary-list">
            <tr><td>Status</td><td>{{ optionalValue status }}</td></tr>
            <tr><td>Status change</td><td>{{ optionalValue statusChange }}</td></tr>
            <tr><td>Status change date</td><td>{{ optionalValue (formatDate statusChangeDate) }}</td></tr>
            <tr><td>Prison name</td><td>{{ optionalValue prisonName }}</td></tr>
            <tr><td>Status change type</td><td>{{ optionalValue prisonName }}</td></tr>
        </table>

        <h3>Support Accepted</h3>
        {{#supportAccepted}}
            <table class="summary-list">
                <tr><td>Modified date</td><td>{{ optionalValue (formatDate modifiedDateTime) }}</td></tr>
            </table>

            <h4>Actions Required</h4>
            {{#actionsRequired}}
                <table class="summary-list">
                    <tr><td>Modified date</td><td>{{ optionalValue (formatDate modifiedDateTime) }}</td></tr>
                </table>
                <h5>Actions</h5>
                {{#actions}}
                    <table class="data-table">
                        <tr>
                            <td class="data-column-25">Id</td>
                            <td class="data-column-25">Todo Item</td>
                            <td class="data-column-25">Status</td>
                            <td class="data-column-25">Other</td>
                        </tr>
                        <tr>
                            <td>{{ optionalValue id }}</td>
                            <td>{{ optionalValue todoItem }}</td>
                            <td>{{ optionalValue status }}</td>
                            <td>{{ optionalValue other }}</td>
                        </tr>
                    </table>
                {{/actions}}
                {{^actions}}
                    <p>No Data Held</p>
                {{/actions}}
            {{/actionsRequired}}
            {{^actionsRequired}}
                <p>No Data Held</p>
            {{/actionsRequired}}

            <h4>Work Impacts</h4>
            {{#workImpacts}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Modified date</td>
                        <td class="data-column-25">Caring responsibilities full time</td>
                        <td class="data-column-25">Able to manage mental health</td>
                        <td class="data-column-25">Able to manage dependencies</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue (formatDate modifiedDateTime) }}</td>
                        <td>{{ convertBoolean caringResponsibilitiesFullTime }}</td>
                        <td>{{ convertBoolean ableToManageMentalHealth }}</td>
                        <td>{{ convertBoolean ableToManageDependencies }}</td>
                    </tr>
                </table>
                <table class="summary-list">
                    <tr><td>Ability to work impacted by</td><td>{{#abilityToWorkImpactedBy}}{{.}}{{#unless @last}}, {{/unless}}{{/abilityToWorkImpactedBy}}{{^abilityToWorkImpactedBy}}No Data Held{{/abilityToWorkImpactedBy}}</td></tr>
                </table>
            {{/workImpacts}}
            {{^workImpacts}}
                <p>No Data Held</p>
            {{/workImpacts}}

            <h4>Work Interests</h4>
            {{#workInterests}}
                <table class="summary-list">
                    <tr><td>Modified date</td><td>{{ optionalValue (formatDate modifiedDateTime) }}</td></tr>
                    <tr><td>Work types of interest</td><td>{{#workTypesOfInterest}}{{.}}{{#unless @last}}, {{/unless}}{{/workTypesOfInterest}}{{^workTypesOfInterest}}No Data Held{{/workTypesOfInterest}}</td></tr>
                    <tr><td>Work types of interest other</td><td>{{ optionalValue workTypesOfInterestOther }}</td></tr>
                    <tr><td>Job of particular interest</td><td>{{ optionalValue jobOfParticularInterest }}</td></tr>
                </table>
            {{/workInterests}}
            {{^workInterests}}
                <p>No Data Held</p>
            {{/workInterests}}

            <h4>Work Experience</h4>
            {{#workExperience}}
                <table class="summary-list">
                    <tr><td>Modified date</td><td>{{ optionalValue (formatDate modifiedDateTime) }}</td></tr>
                    <tr><td>Qualifications and training</td><td>{{#qualificationsAndTraining}}{{.}}{{#unless @last}}, {{/unless}}{{/qualificationsAndTraining}}{{^qualificationsAndTraining}}No Data Held{{/qualificationsAndTraining}}</td></tr>
                    <tr><td>Previous work or volunteering</td><td>{{ optionalValue previousWorkOrVolunteering }}</td></tr>
                    <tr><td>Qualifications and training other</td><td>{{ optionalValue qualificationsAndTrainingOther }}</td></tr>
                </table>
            {{/workExperience}}
            {{^workExperience}}
                <p>No Data Held</p>
            {{/workExperience}}
        {{/supportAccepted}}
        {{^supportAccepted}}
            <p>No Data Held</p>
        {{/supportAccepted}}

        <h3>Support Declined</h3>
        {{#supportDeclined}}

        {{/supportDeclined}}
        {{^supportDeclined}}
            <p>No Data Held</p>
        {{/supportDeclined}}

        <h3>Support Accepted History</h3>
