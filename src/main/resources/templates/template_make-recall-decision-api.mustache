            <tr><td class="data-column-20">Has victims in contact scheme</td></tr>
            {{#selected}}
                <tr><td>{{ optionalValue selected }}</td></tr>
            {{/selected}}
        </table>
        <table class="data-table">
            <tr>
                <td class="data-column-35">All options</td>
                <td class="data-column-65"></td>
            </tr>
            {{#allOptions}}
                <tr>
                    <td>{{ optionalValue value }}</td>
                    <td>{{ optionalValue text }}</td>
                </tr>
            {{/allOptions}}
        </table>
    {{/hasVictimsInContactScheme}}
        <br>
    {{#indeterminateSentenceType}}
