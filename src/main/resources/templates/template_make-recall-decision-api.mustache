                        <td class="data-column-35">Sub Category code</td>
                    </tr>
                    {{#allOptions}}
                        <tr>
                            <td>{{ optionalValue title }}</td>
                            <td>{{ optionalValue mainCatCode }}</td>
                            <td>{{ optionalValue subCatCode }}</td>
                        </tr>
                    {{/allOptions}}
                    {{^allOptions}}
                        <tr>
                            <td>No Data Held</td>
                            <td>No Data Held</td>
                            <td>No Data Held</td>
                        </tr>
                    {{/allOptions}}
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-65">Details</td>
                        <td class="data-column-35">Note</td>
                    </tr>
                    {{#allOptions}}
                        <tr>
                            <td>{{ optionalValue details }}</td>
                            <td>{{ optionalValue note }}</td>
                        </tr>
                    {{/allOptions}}
                    {{^allOptions}}
                        <tr>
                            <td>No Data Held</td>
                            <td>No Data Held</td>
                        </tr>
                    {{/allOptions}}
                </table>
            {{/additionalLicenceConditions}}
        {{/licenceConditionsBreached}}
        {{^licenceConditionsBreached}}
            <p>No Data Held</p>
        {{/licenceConditionsBreached}}

