<h1>Consider a Recall</h1>
<br>
{{#.}}
    <table class="summary-list">
        <tr><td>CRN</td><td>{{ optionalValue crn }}</td></tr>
    </table>
    <br>
    <h3>Recommendations</h3>
    <br>
    {{#recommendations}}
        <table class="data-table">
            <tr>
                <td class="data-column-35">CRN</td>
                <td class="data-column-30">Sensitive</td>
                <td class="data-column-35">PPUD record present</td>
            </tr>
            <tr>
                <td>{{ optionalValue crn }}</td>
                <td>{{ optionalValue sensitive }}</td>
                <td>{{ optionalValue ppudRecordPresent }}</td>
            </tr>
        </table>
        <br>
        <h5>Recall considered list</h5>
        {{#recallConsideredList}}
        <table class="data-table">
            <tr>
                <td class="data-column-35">Date created</td>
                <td class="data-column-30">User ID</td>
                <td class="data-column-35"></td>
            </tr>
            <tr>
                <td>{{ optionalValue (formatDate createdDate) }}</td>
                <td>{{ getUserLastName userId }}</td>
                <td></td>
            </tr>
        </table>
        <table class="data-table">
            <tr>
                <td class="data-column-100">Details</td>
            </tr>
            <tr>
                <td>{{ optionalValue recallConsideredDetail }}</td>
            </tr>
        </table>
        {{/recallConsideredList}}
        <br>
        {{#recallType}}
            <table class="data-table">
                <tr>
                    <td class="data-column-20">Recall type</td>
                    <td class="data-column-80">Details</td>
                </tr>
            {{#selected}}
                <tr>
                    <td>{{ optionalValue value }}</td>
                    <td>{{ optionalValue details }}</td>
                </tr>
            {{/selected}}
            </table>
            <table class="data-table">
                <tr>
                    <td class="data-column-35">All options</td>
                    <td class="data-column-65"></td>
                </tr>
                {{#allOptions}}
                <tr>
                    <td>{{ optionalValue value }}</td>
                    <td>{{ optionalValue text }}</td>
                </tr>
                {{/allOptions}}
            </table>
        {{/recallType}}
    <br>
    {{#managerRecallDecision}}
        <table class="data-table">
            <tr>
                <td class="data-column-20">Manager recall decision</td>
                <td class="data-column-80">Details</td>
            </tr>
            {{#selected}}
                <tr>
                    <td>{{ optionalValue value }}</td>
                    <td>{{ optionalValue details }}</td>
                </tr>
            {{/selected}}
        </table>
        <table class="data-table">
            <tr>
                <td class="data-column-35">All options</td>
                <td class="data-column-65"></td>
            </tr>
            {{#allOptions}}
                <tr>
                    <td>{{ optionalValue value }}</td>
                    <td>{{ optionalValue text }}</td>
                </tr>
            {{/allOptions}}
        </table>
    {{/managerRecallDecision}}

    <br>
    <table class="data-table">
        <tr>
            <td class="data-column-35">Send SPO Rationale to Delius</td>
            <td class="data-column-30">Is sent to Delius</td>
            <td class="data-column-35">Date created</td>
        </tr>
        <tr>
            <td>{{ optionalValue sendSpoRationaleToDelius }}</td>
            <td>{{ optionalValue isSentToDelius }}</td>
            <td>{{ optionalValue (formatDate createdDate) }}</td>
        </tr>
    </table>

    {{#considerationRationale}}
        <br>
        <h5>Consideration Rationale</h5>
        <table class="data-table">
            <tr>
                <td class="data-column-35">Date created</td>
                <td class="data-column-30">Time created</td>
                <td class="data-column-35">Sensitive</td>
            </tr>
            <tr>
                <td>{{ optionalValue (formatDate createdDate) }}</td>
                <td>{{ optionalValue (formatDate createdTime) }}</td>
                <td>{{ optionalValue sensitive }}</td>
            </tr>
        </table>
    {{/considerationRationale}}

    {{#custodyStatus}}
        <br>
        <h5>Custody status</h5>
        <table class="data-table">
            <tr>
                <td class="data-column-35">Code</td>
                <td class="data-column-65">Description</td>
            </tr>
            <tr>
                <td>{{ optionalValue code }}</td>
                <td>{{ optionalValue description }}</td>
            </tr>
        </table>
    {{/custodyStatus}}

    {{#localPoliceContact}}
