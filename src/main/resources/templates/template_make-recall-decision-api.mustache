                <table class="data-table">
                    <tr>
                        <td class="data-column-20">Standard licence conditions</td>
                    </tr>
                        <tr>
                            <td>{{#selected}}<li>{{ optionalValue . }}</li>{{/selected}}</td>
                        </tr>
                </table>
                <table class="data-table">
