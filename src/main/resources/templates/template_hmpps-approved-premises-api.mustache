                            <td>{{ optionalValue canClimbStairs }}</td>
                            <td>{{ optionalValue isReceivingMedicationOrTreatment }}</td>
                            <td>{{ optionalValue hasPhyHealthNeeds }}</td>
                            <td>{{ optionalValue requiresAdditionalSupport }}</td>
