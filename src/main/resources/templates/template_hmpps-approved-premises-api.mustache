                    </table>
                    <table class="summary-list">
                        <tr><td>Independent living detail</td><td>{{ optionalValue indyLivingDetail }}</td></tr>
                        <tr><td>Medication or treatment detail</td><td>{{ optionalValue medicationOrTreatmentDetail }}</td></tr>
                        <tr><td>Physical health needs detail</td><td>{{ optionalValue needsDetail }}</td></tr>
