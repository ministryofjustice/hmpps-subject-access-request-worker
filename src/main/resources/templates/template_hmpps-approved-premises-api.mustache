                    <tr><td>Family property</td><td>{{ optionalValue family-accommodation.familyProperty }}</td></tr>
                </table>
                <p><b>First preferred area</b></p>
                <table class="summary-list">
                    <tr><td>Preferred area</td><td>{{ optionalValue first-preferred-area.preferredArea }}</td></tr>
                    <tr><td>Preference reason</td><td>{{ optionalValue first-preferred-area.preferenceReason }}</td></tr>
