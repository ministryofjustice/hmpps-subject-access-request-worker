                        <td class="data-column-25">White background</td>
                        <td class="data-column-25">Optional white background</td>
                        <td class="data-column-25">Sexual orientation</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue disability.otherDisability }}</td>
                        <td>{{ optionalValue white-background.whiteBackground }}</td>
                        <td>{{ optionalValue white-background.optionalWhiteBackground }}</td>
