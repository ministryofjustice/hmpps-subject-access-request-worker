                <p><b>Second preferred area</b></p>
                <table class="summary-list">
                    <tr><td>Preferred area</td><td>{{ optionalValue second-preferred-area.preferredArea }}</td></tr>
                    <tr><td>Preference reason</td><td>{{ optionalValue second-preferred-area.preferenceReason }}</td></tr>
                </table>
                <p><b>Exclusion zones</b></p>
                <table class="summary-list">
                    <tr><td>Has exclusion zones</td><td>{{ optionalValue exclusion-zones.hasExclusionZones }}</td></tr>
                    <tr><td>Exclusion zones detail</td><td>{{ optionalValue exclusion-zones.exclusionZonesDetail }}</td></tr>
                </table>
                <p><b>Gang affiliations</b></p>
                <table class="summary-list">
                    <tr><td>Has gang affiliations</td><td>{{ optionalValue gang-affiliations.hasGangAffiliations }}</td></tr>
                    <tr><td>Gang name</td><td>{{ optionalValue gang-affiliations.gangName }}</td></tr>
                    <tr><td>Gang operation area</td><td>{{ optionalValue gang-affiliations.gangOperationArea }}</td></tr>
                    <tr><td>Rival gang detail</td><td>{{ optionalValue gang-affiliations.rivalGangDetail }}</td></tr>
                </table>
            {{/data.area-information}}
            {{^data.area-information}}
                <p>No Data Held</p>
            {{/data.area-information}}

            <h5>Health needs</h5>
