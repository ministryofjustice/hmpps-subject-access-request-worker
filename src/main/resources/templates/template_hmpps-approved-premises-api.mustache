                        <tr><td>Has additional information</td><td>{{ optionalValue hasAdditionalInformation }}</td></tr>
                        <tr><td>Additional information detail</td><td>{{ optionalValue additionalInformationDetail }}</td></tr>
                    </table>
                {{/additional-information}}
                {{^additional-information}}
                    <p>No Data Held</p>
                {{/additional-information}}

                <p><b>Vulnerability</b></p>
                {{#vulnerability}}
