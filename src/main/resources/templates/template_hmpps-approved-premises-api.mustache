                        <tr><td>Medication issues</td><td>{{ optionalValue medicationIssues }}</td></tr>
                        <tr><td>Can manage medication notes</td><td>{{ optionalValue canManageMedicationNotes }}</td></tr>
                        <tr><td>Can't manage medication notes</td><td>{{ optionalValue cantManageMedicationNotes }}</td></tr>
                    </table>
                {{/mental-health}}
                {{^mental-health}}
                    <p>No Data Held</p>
                {{/mental-health}}

                <p><b>Mental health</b></p>
