<h1>Approved Premises</h1>
{{#.}}
    {{#ShortTermAccommodation}}
    <h2>Short term accommodation</h2>

        <h3>Status update details</h3>
        {{#StatusUpdateDetails}}
            <table class="data-table">
                <tr>
                    <td class="data-column-30">Status Label</td>
                    <td class="data-column-30">Created at</td>
                    <td class="data-column-30">Detail Label</td>
                </tr>
                <tr>
                    <td>{{ optionalValue status_label }}</td>
                    <td>{{ optionalValue (formatDate created_at) }}</td>
                    <td>{{ optionalValue detail_label }}</td>
                </tr>
            </table>
        {{/StatusUpdateDetails}}
        {{^StatusUpdateDetails}}
            <p>No Data Held</p>
        {{/StatusUpdateDetails}}

        <h3>Applications</h3>
        {{#Applications}}
            <h4>Application</h4>
            <br/>
            <table class="summary-list">
                <tr><td>Submitted at</td><td>{{ optionalValue (formatDate submitted_at) }}</td></tr>
                <tr><td>Conditional release date</td><td>{{ optionalValue (formatDate conditional_release_date) }}</td></tr>
                <tr><td>Created at</td><td>{{ optionalValue (formatDate created_at) }}</td></tr>
                <tr><td>Abandoned at</td><td>{{ optionalValue (formatDate abandoned_at) }}</td></tr>
                <tr><td>Referring prison code</td><td>{{ getPrisonName referring_prison_code }}</td></tr>
                <tr><td>Preferred areas</td><td>{{ optionalValue preferred_areas }}</td></tr>
                <tr><td>Home Detention Curfew eligibility date</td><td>{{ optionalValue (formatDate hdc_eligibility_date) }}</td></tr>
            </table>

            <h5>Address History</h5>
            {{#data.address-history.previous-address}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-50">Previous address line 1</td>
                        <td class="data-column-50">Previous address line 2</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue previousAddressLine1 }}</td>
                        <td>{{ optionalValue previousAddressLine2 }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-30">Previous town or city</td>
                        <td class="data-column-30">Previous county</td>
                        <td class="data-column-30">Previous postcode</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue previousTownOrCity }}</td>
                        <td>{{ optionalValue previousCounty }}</td>
                        <td>{{ optionalValue previousPostcode }}</td>
                    </tr>
                </table>
            {{/data.address-history.previous-address}}
            {{^data.address-history.previous-address}}
                <p>No Data Held</p>
            {{/data.address-history.previous-address}}

            <h5>Offending history</h5>
            {{#data.offending-history}}
                <table class="summary-list">
                    <tr><td>Has any previous convictions</td><td>{{ convertCamelCase any-previous-convictions.hasAnyPreviousConvictions }}</td></tr>
                </table>
                <p><b>Offence history</b></p>
                {{#offence-history-data}}
                    <p><b>Offence</b></p>
                    <table class="summary-list">
                        <tr><td>Title</td><td>{{ optionalValue titleAndNumber }}</td></tr>
                        <tr><td>Offence date</td><td>{{ buildDate offenceDate-year offenceDate-month offenceDate-day }}</td></tr>
                        <tr><td>Offence category</td><td>{{ optionalValue offenceCategory }}</td></tr>
                        <tr><td>Sentence length</td><td>{{ optionalValue sentenceLength }}</td></tr>
                        <tr><td>Summary</td><td>{{ optionalValue summary }}</td></tr>
                    </table>
                {{/offence-history-data}}
                {{^offence-history-data}}
                    <p>No Data Held</p>
                {{/offence-history-data}}
            {{/data.offending-history}}
            {{^data.offending-history}}
                <p>No Data Held</p>
            {{/data.offending-history}}

            <h5>Risk to self</h5>
            {{#data.risk-to-self}}

                <p><b>Historical risk</b></p>
                {{#historical-risk}}
                    <table class="summary-list">
                        <tr><td>Historical risk detail</td><td>{{ optionalValue historicalRiskDetail }}</td></tr>
                        <tr><td>Confirmation</td><td>{{ optionalValue confirmation }}</td></tr>
                    </table>
                {{/historical-risk}}
                {{^historical-risk}}
                    <p>No Data Held</p>
                {{/historical-risk}}

                <p><b>Additional information</b></p>
                {{#additional-information}}
                    <table class="summary-list">
                        <tr><td>Has additional information</td><td>{{ optionalValue hasAdditionalInformation }}</td></tr>
                        <tr><td>Additional information detail</td><td>{{ optionalValue additionalInformationDetail }}</td></tr>
                    </table>
                {{/additional-information}}
                {{^additional-information}}
                    <p>No Data Held</p>
                {{/additional-information}}

                <p><b>Vulnerability</b></p>
                {{#vulnerability}}
                    <table class="summary-list">
                        <tr><td>Vulnerability detail</td><td>{{ optionalValue vulnerabilityDetail }}</td></tr>
                        <tr><td>Confirmation</td><td>{{ optionalValue confirmation }}</td></tr>
                    </table>
                {{/vulnerability}}
                {{^vulnerability}}
                    <p>No Data Held</p>
                {{/vulnerability}}

                <p><b>Current risk</b></p>
                {{#current-risk}}
                    <table class="summary-list">
                        <tr><td>Current risk detail</td><td>{{ optionalValue currentRiskDetail }}</td></tr>
                        <tr><td>Confirmation</td><td>{{ optionalValue confirmation }}</td></tr>
                    </table>
                {{/current-risk}}
                {{^current-risk}}
                    <p>No Data Held</p>
                {{/current-risk}}

            {{/data.risk-to-self}}
            {{^data.risk-to-self}}
                <p>No Data Held</p>
            {{/data.risk-to-self}}

            <h5>Community Probation Practitioner details and Home Detention Curfew licence conditions</h5>
            {{#data.cpp-details-and-hdc-licence-conditions}}
                <p><b>Community Probation Practitioner details</b></p>
                {{#cpp-details}}
                    <table class="data-table">
                        <tr>
                            <td class="data-column-25">Name</td>
                            <td class="data-column-25">Probation region</td>
                            <td class="data-column-25">Telephone</td>
                            <td class="data-column-25">Email</td>
                        </tr>
                        <tr>
                            <td>{{ optionalValue name }}</td>
                            <td>{{ optionalValue probationRegion }}</td>
                            <td>{{ optionalValue telephone }}</td>
                            <td>{{ optionalValue email }}</td>
                        </tr>
                    </table>
                {{/cpp-details}}
                {{^cpp-details}}
                    <p>No Data Held</p>
                {{/cpp-details}}
                <p><b>Non-standard licence conditions</b></p>
                {{#non-standard-licence-conditions}}
                    <table class="summary-list">
                        <tr><td>Non-standard licence conditions</td><td>{{ optionalValue nonStandardLicenceConditions }}</td></tr>
                        <tr><td>Non-standard licence conditions detail</td><td>{{ optionalValue nonStandardLicenceConditionsDetail }}</td></tr>
                    </table>
                {{/non-standard-licence-conditions}}
                {{^non-standard-licence-conditions}}
                    <p>No Data Held</p>
                {{/non-standard-licence-conditions}}
            {{/data.cpp-details-and-hdc-licence-conditions}}
            {{^data.cpp-details-and-hdc-licence-conditions}}
                <p>No Data Held</p>
            {{/data.cpp-details-and-hdc-licence-conditions}}

            <h5>Information needed from applicant</h5>
            {{#data.information-needed-from-applicant.information-needed-from-applicant}}
                <table class="summary-list">
                    <tr><td>Has information needed</td><td>{{ optionalValue hasInformationNeeded }}</td></tr>
                </table>
            {{/data.information-needed-from-applicant.information-needed-from-applicant}}
            {{^data.information-needed-from-applicant.information-needed-from-applicant}}
                <p>No Data Held</p>
            {{/data.information-needed-from-applicant.information-needed-from-applicant}}

            <h5>Confirm consent</h5>
            {{#data.confirm-consent.confirm-consent}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-50">Has given consent</td>
                        <td class="data-column-50">Consent date</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue hasGivenConsent }}</td>
                        <td>{{ optionalValue (formatDate consentDate) }}</td>
                    </tr>
                </table>
            {{/data.confirm-consent.confirm-consent}}
            {{^data.confirm-consent.confirm-consent}}
                <p>No Data Held</p>
            {{/data.confirm-consent.confirm-consent}}

            <h5>Funding information</h5>
            {{#data.funding-information}}
