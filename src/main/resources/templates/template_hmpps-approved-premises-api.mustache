                    <tr>
                        <td class="data-column-50">National insurance number</td>
                        <td class="data-column-50">Funding source</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue national-insurance.nationalInsuranceNumber }}</td>
                        <td>{{ convertCamelCase funding-source.fundingSource }}</td>
                    </tr>
                </table>
            {{/data.funding-information}}
            {{^data.funding-information}}
                <p>No Data Held</p>
            {{/data.funding-information}}

            <h5>Confirm eligibility</h5>
            {{#data.confirm-eligibility}}
            <table class="summary-list">
                <tr><td>Is eligible</td><td>{{ optionalValue confirm-eligibility.isEligible }}</td></tr>
            </table>
            {{/data.confirm-eligibility}}
            {{^data.confirm-eligibility}}
                <p>No Data Held</p>
            {{/data.confirm-eligibility}}

            <h5>Current offences</h5>
            {{#data.current-offences.current-offence-data}}
                <p><b>Offence</b></p>
                <table class="summary-list">
                    <tr><td>Title</td><td>{{ optionalValue titleAndNumber }}</td></tr>
                    <tr><td>Offence date</td><td>{{ buildDate offenceDate-year offenceDate-month offenceDate-day }}</td></tr>
                    <tr><td>Offence category</td><td>{{ optionalValue offenceCategory }}</td></tr>
                    <tr><td>Sentence length</td><td>{{ optionalValue sentenceLength }}</td></tr>
                    <tr><td>Summary</td><td>{{ optionalValue summary }}</td></tr>
                    <tr><td>Outstanding charges</td><td>{{ optionalValue outstandingCharges }}</td></tr>
                    <tr><td>Outstanding charges detail</td><td>{{ optionalValue outstandingChargesDetail }}</td></tr>
                </table>
            {{/data.current-offences.current-offence-data}}
            {{^data.current-offences.current-offence-data}}
                <p>No Data Held</p>
            {{/data.current-offences.current-offence-data}}

            <h5>Personal information</h5>
            {{#data.personal-information}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-30">Has working mobile phone</td>
                        <td class="data-column-30">Mobile phone number</td>
                        <td class="data-column-30">Is smartphone</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue working-mobile-phone.working-mobile-phone }}</td>
                        <td>{{ optionalValue working-mobile-phone.mobilePhoneNumber }}</td>
                        <td>{{ optionalValue working-mobile-phone.isSmartPhone }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-30">Immigration status</td>
                        <td class="data-column-30">Is pregnant</td>
                        <td class="data-column-30">Pregnancy due date</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue immigration-status.immigrationStatus }}</td>
                        <td>{{ optionalValue pregnancy-information.isPregnant }}</td>
                        <td>{{ buildDate pregnancy-information.dueDate-year pregnancy-information.dueDate-month pregnancy-information.dueDate-day }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-30">Has support worker preference</td>
                        <td class="data-column-30">Support worker preference</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue support-worker-preference.hasSupportWorkerPreference }}</td>
                        <td>{{ optionalValue support-worker-preference.supportWorkerPreference }}</td>
                    </tr>
                </table>
            {{/data.personal-information}}
            {{^data.personal-information}}
                <p>No Data Held</p>
            {{/data.personal-information}}

            <h5>Check your answers</h5>
            {{#data.check-your-answers}}
                <table class="summary-list">
                    <tr><td>Check your answers</td><td>{{ optionalValue check-your-answers.checkYourAnswers }}</td></tr>
                </table>
            {{/data.check-your-answers}}
            {{^data.check-your-answers}}
                <p>No Data Held</p>
            {{/data.check-your-answers}}

            <h5>Equality and diversity monitoring</h5>
            {{#data.equality-and-diversity-monitoring}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Is care leaver</td>
                        <td class="data-column-25">Is veteran</td>
                        <td class="data-column-25">Gender</td>
                        <td class="data-column-25">Sex</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue care-leaver.isCareLeaver }}</td>
                        <td>{{ optionalValue military-veteran.isVeteran }}</td>
                        <td>{{ optionalValue sex-and-gender.gender }}</td>
                        <td>{{ optionalValue sex-and-gender.sex }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Optional gender identity</td>
                        <td class="data-column-25">Ethnic group</td>
                        <td class="data-column-25">Has parental or carer responsibilities</td>
                        <td class="data-column-25">Has disability</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue sex-and-gender.optionalGenderIdentity }}</td>
                        <td>{{ optionalValue ethnic-group.ethnicGroup }}</td>
                        <td>{{ optionalValue parental-carer-responsibilities.hasParentalOrCarerResponsibilities }}</td>
                        <td>{{ convertCamelCase disability.hasDisability }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Other disability</td>
                        <td class="data-column-25">White background</td>
                        <td class="data-column-25">Optional white background</td>
                        <td class="data-column-25">Sexual orientation</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue disability.otherDisability }}</td>
                        <td>{{ optionalValue white-background.whiteBackground }}</td>
                        <td>{{ optionalValue white-background.optionalWhiteBackground }}</td>
                        <td>{{ convertCamelCase sexual-orientation.orientation }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Other sexual orientation</td>
                        <td class="data-column-25">Marital status</td>
                        <td class="data-column-25">Will answer equality questions</td>
                        <td class="data-column-25">Religion</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue sexual-orientation.otherOrientation }}</td>
                        <td>{{ convertCamelCase marital-status.maritalStatus }}</td>
                        <td>{{ optionalValue will-answer-equality-questions.willAnswer }}</td>
                        <td>{{ optionalValue religion.religion }}</td>
                    </tr>
                </table>
            {{/data.equality-and-diversity-monitoring}}
            {{^data.equality-and-diversity-monitoring}}
                <p>No Data Held</p>
            {{/data.equality-and-diversity-monitoring}}

            <h5>Risk of serious harm</h5>
            {{#data.risk-of-serious-harm}}
                <p><b>Summary</b></p>
                <table class="summary-list">
                    <tr><td>Additional comments</td><td>{{ optionalValue summary.additionalComments }}</td></tr>
                </table>

                <p><b>Reducing risk</b></p>
                <table class="summary-list">
                    <tr><td>Factors likely to reduce risk</td><td>{{ optionalValue reducing-risk.factorsLikelyToReduceRisk }}</td></tr>
                    <tr><td>Confirmation</td><td>{{ optionalValue reducing-risk.confirmation }}</td></tr>
                </table>

                <p><b>Cell share information</b></p>
                <table class="summary-list">
                    <tr><td>Has cell share comments</td><td>{{ optionalValue cell-share-information.hasCellShareComments }}</td></tr>
                    <tr><td>Cell share information detail</td><td>{{ optionalValue cell-share-information.cellShareInformationDetail }}</td></tr>
                </table>

                <p><b>Summary data</b></p>
                {{#summary-data}}
                    <table class="data-table">
                        <tr>
                            <td class="data-column-30">Last updated</td>
                            <td class="data-column-30">Overall risk</td>
                            <td class="data-column-30">Risk to children</td>
                        </tr>
                        <tr>
                            <td>{{ optionalValue (formatDate value.lastUpdated) }}</td>
                            <td>{{ optionalValue value.overallRisk }}</td>
                            <td>{{ optionalValue value.riskToChildren }}</td>
                        </tr>
                    </table>
                    <table class="data-table">
                        <tr>
                            <td class="data-column-30">Risk to public</td>
                            <td class="data-column-30">Risk to known adult</td>
                            <td class="data-column-30">Risk to staff</td>
                        </tr>
                        <tr>
                            <td>{{ optionalValue value.riskToPublic }}</td>
                            <td>{{ optionalValue value.riskToKnownAdult }}</td>
                            <td>{{ optionalValue value.riskToStaff }}</td>
                        </tr>
                    </table>
                {{/summary-data}}
                {{^summary-data}}
                    <p>No Data Held</p>
                {{/summary-data}}

                <p><b>Risk factors</b></p>
                {{#risk-factors}}
                    <table class="summary-list">
                        <tr><td>Circumstances likely to increase risk</td><td>{{ optionalValue circumstancesLikelyToIncreaseRisk }}</td></tr>
                        <tr><td>When is risk likely to be greatest</td><td>{{ optionalValue whenIsRiskLikelyToBeGreatest }}</td></tr>
                        <tr><td>Confirmation</td><td>{{ optionalValue confirmation }}</td></tr>
                    </table>
                {{/risk-factors}}
                {{^risk-factors}}
