                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-30">Has support worker preference</td>
                        <td class="data-column-30">Support worker preference</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue support-worker-preference.hasSupportWorkerPreference }}</td>
                        <td>{{ optionalValue support-worker-preference.supportWorkerPreference }}</td>
                    </tr>
                </table>
            {{/data.personal-information}}
            {{^data.personal-information}}
