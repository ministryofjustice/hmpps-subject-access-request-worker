                    <tr><td>Offence category</td><td>{{ optionalValue offenceCategory }}</td></tr>
                    <tr><td>Sentence length</td><td>{{ optionalValue sentenceLength }}</td></tr>
                    <tr><td>Summary</td><td>{{ optionalValue summary }}</td></tr>
                    <tr><td>Outstanding charges</td><td>{{ optionalValue outstandingCharges }}</td></tr>
                    <tr><td>Outstanding charges detail</td><td>{{ optionalValue outstandingChargesDetail }}</td></tr>
                </table>
            {{/data.current-offences.current-offence-data}}
            {{^data.current-offences.current-offence-data}}
