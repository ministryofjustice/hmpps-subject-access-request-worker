                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Other sexual orientation</td>
                        <td class="data-column-25">Marital status</td>
                        <td class="data-column-25">Will answer equality questions</td>
                        <td class="data-column-25">Religion</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue sexual-orientation.otherOrientation }}</td>
                        <td>{{ optionalValue marital-status.maritalStatus }}</td>
                        <td>{{ optionalValue will-answer-equality-questions.willAnswer }}</td>
                        <td>{{ optionalValue religion.religion }}</td>
                    </tr>
