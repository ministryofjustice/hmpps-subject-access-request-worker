                        <td>{{ optionalValue sex-and-gender.gender }}</td>
                        <td>{{ optionalValue sex-and-gender.sex }}</td>
                    </tr>
                </table>
                <table class="data-table">
                    <tr>
                        <td class="data-column-25">Optional gender identity</td>
                        <td class="data-column-25">Ethnic group</td>
                        <td class="data-column-25">Has parental or carer responsibilities</td>
                        <td class="data-column-25">Has disability</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue sex-and-gender.optionalGenderIdentity }}</td>
                        <td>{{ optionalValue ethnic-group.ethnicGroup }}</td>
                        <td>{{ optionalValue parental-carer-responsibilities.hasParentalOrCarerResponsibilities }}</td>
