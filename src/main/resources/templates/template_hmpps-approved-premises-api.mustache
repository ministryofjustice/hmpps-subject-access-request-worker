                {{/risk-factors}}

                <p><b>Risk management arrangements</b></p>
                {{#risk-management-arrangements}}
                    <table class="summary-list">
                        <tr><td>Arrangements</td><td>{{ optionalValue arrangements }}</td></tr>
                        <tr><td>Multi Agency Risk Assessment Conference details</td><td>{{ optionalValue maracDetails }}</td></tr>
                        <tr><td>Multi Agency Public Protection Arrangements details</td><td>{{ optionalValue mappaDetails }}</td></tr>
                        <tr><td>Integrated Offender Management details</td><td>{{ optionalValue iomDetails }}</td></tr>
                    </table>
                {{/risk-management-arrangements}}
                {{^risk-management-arrangements}}
                    <p>No Data Held</p>
                {{/risk-management-arrangements}}

                <p><b>Additional risk information</b></p>
                {{#additional-risk-information}}
                    <table class="summary-list">
                        <tr><td>Has additional risk information</td><td>{{ optionalValue hasAdditionalInformation }}</td></tr>
                        <tr><td>Additional information detail</td><td>{{ optionalValue additionalInformationDetail }}</td></tr>
                    </table>
                {{/additional-risk-information}}
                {{^additional-risk-information}}
                    <p>No Data Held</p>
                {{/additional-risk-information}}

                <p><b>Risk to others</b></p>
                {{#risk-to-others}}
                    <table class="summary-list">
                        <tr><td>Who is at risk</td><td>{{ optionalValue whoIsAtRisk }}</td></tr>
                        <tr><td>Nature of risk</td><td>{{ optionalValue natureOfRisk }}</td></tr>
                        <tr><td>Confirmation</td><td>{{ optionalValue confirmation }}</td></tr>
                    </table>
                {{/risk-to-others}}
                {{^risk-to-others}}
                    <p>No Data Held</p>
                {{/risk-to-others}}

            {{/data.risk-of-serious-harm}}
            {{^data.risk-of-serious-harm}}
                <p>No Data Held</p>
            {{/data.risk-of-serious-harm}}

            <h5>Home Detention Curfew Licence dates</h5>
            {{#data.hdc-licence-dates}}
                <table class="data-table">
                    <tr>
                        <td class="data-column-50">Home Detention Curfew eligibility date</td>
                        <td class="data-column-50">Conditional release date</td>
                    </tr>
                    <tr>
                        <td>{{ optionalValue (formatDate hdc-licence-dates.hdcEligibilityDate) }}</td>
                        <td>{{ optionalValue (formatDate hdc-licence-dates.conditionalReleaseDate) }}</td>
                    </tr>
                </table>
            {{/data.hdc-licence-dates}}
            {{^data.hdc-licence-dates}}
