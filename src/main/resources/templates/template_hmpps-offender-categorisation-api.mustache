                    <tr><td>Category B is warranted</td><td>{{ optionalValue categoryBIsWarranted }}</td></tr>
                </table>
            {{/categoryDecisionBasedOnSecurityInformation}}
            {{^categoryDecisionBasedOnSecurityInformation}}
                <p>No Data Held</p>
            {{/categoryDecisionBasedOnSecurityInformation}}

        {{/ratings}}
        {{^ratings}}
            <p>No Data Held</p>
        {{/ratings}}

