<h1>Prisoner categorisation</h1>

<h2>Categorisation forms</h2>
{{#catForm}}
    <h3>Categorisation form</h3>
    <table class="data-table">
        <tr>
            <td class="data-column-25">Prison name</td>
            <td class="data-column-25">Start date</td>
            <td class="data-column-25">Security reviewed date</td>
            <td class="data-column-25">Approval date</td>
        </tr>
        <tr>
            <td>{{ getPrisonName prisonId }}</td>
            <td>{{ optionalValue (formatDate start_date) }}</td>
            <td>{{ optionalValue (formatDate security_reviewed_date) }}</td>
            <td>{{ optionalValue (formatDate approval_date) }}</td>
        </tr>
    </table>
    <table class="data-table">
        <tr>
            <td class="data-column-25">Assessment date</td>
            <td class="data-column-25">Due by date</td>
            <td class="data-column-25">Cancelled date</td>
            <td class="data-column-25">Type of categorisation assessment</td>
        </tr>
        <tr>
            <td>{{ optionalValue (formatDate assessment_date) }}</td>
            <td>{{ optionalValue (formatDate due_by_date) }}</td>
            <td>{{ optionalValue (formatDate cancelled_date) }}</td>
            <td>{{ optionalValue typeOfCategorisationAssessment }}</td>
        </tr>
    </table>
    <table class="data-table">
        <tr>
            <td class="data-column-50">Review status</td>
            <td class="data-column-50">Review reason</td>
        </tr>
        <tr>
            <td>{{ optionalValue reviewStatus }}</td>
            <td>{{ optionalValue reviewReason }}</td>
        </tr>
    </table>

    <h4>Form Response</h4>
    {{#form_response}}

        <h5>Ratings</h5>
        {{#ratings}}

            <p><b>Escape Rating</b></p>
            {{#escapeRating}}
                <table class="summary-list">
                    <tr><td>Category B</td><td>{{ optionalValue escapeCatB }}</td></tr>
                    <tr><td>Category B text</td><td>{{ optionalValue escapeCatBText }}</td></tr>
                    <tr><td>Other evidence</td><td>{{ optionalValue escapeOtherEvidence }}</td></tr>
                    <tr><td>Other evidence text</td><td>{{ optionalValue escapeOtherEvidenceText }}</td></tr>
                </table>
            {{/escapeRating}}
            {{^escapeRating}}
                <p>No Data Held</p>
            {{/escapeRating}}

            <p><b>Extremism Rating</b></p>
            {{#extremismRating}}
                <table class="summary-list">
                    <tr><td>Previous terrorism offences</td><td>{{ optionalValue previousTerrorismOffences }}</td></tr>
                    <tr><td>Text</td><td>{{ optionalValue previousTerrorismOffencesText }}</td></tr>
                </table>
            {{/extremismRating}}
            {{^extremismRating}}
                <p>No Data Held</p>
            {{/extremismRating}}

            <p><b>Violence Rating</b></p>
            {{#violenceRating}}
                <table class="summary-list">
                    <tr><td>Serious threat</td><td>{{ optionalValue seriousThreat }}</td></tr>
                    <tr><td>Serious threat text</td><td>{{ optionalValue seriousThreatText }}</td></tr>
                    <tr><td>High risk of violence</td><td>{{ optionalValue highRiskOfViolence }}</td></tr>
                    <tr><td>High risk of violence text</td><td>{{ optionalValue highRiskOfViolenceText }}</td></tr>
                </table>
            {{/violenceRating}}
            {{^violenceRating}}
                <p>No Data Held</p>
            {{/violenceRating}}

            <p><b>Offending history</b></p>
            {{#offendingHistory}}
                <table class="summary-list">
                    <tr><td>Previous convictions</td><td>{{ optionalValue previousConvictions }}</td></tr>
                    <tr><td>Previous convictions text</td><td>{{ optionalValue previousConvictionsText }}</td></tr>
                    <tr><td>Further charges</td><td>{{ optionalValue this.furtherCharges }}</td></tr>
                    <tr><td>Further charges text</td><td>{{ optionalValue furtherChargesText }}</td></tr>
                    <tr><td>Category B offending history</td><td>{{ optionalValue offendingHistoryCatB }}</td></tr>
                </table>
            {{/offendingHistory}}
            {{^offendingHistory}}
                <p>No Data Held</p>
            {{/offendingHistory}}

            <p><b>Security Input</b></p>
            {{#securityInput}}
                <table class="summary-list">
                    <tr><td>Security input needed</td><td>{{ optionalValue securityInputNeeded }}</td></tr>
                    <tr><td>Text</td><td>{{ optionalValue securityInputNeededText }}</td></tr>
                </table>
            {{/securityInput}}
            {{^securityInput}}
                <p>No Data Held</p>
            {{/securityInput}}

            <p><b>Security back</b></p>
            {{#securityBack}}
                <table class="summary-list">
                    <tr><td>Cat B</td><td>{{ optionalValue catB }}</td></tr>
                </table>
            {{/securityBack}}
            {{^securityBack}}
