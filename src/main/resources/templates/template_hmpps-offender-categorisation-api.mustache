                    <tr><td>Date</td><td>{{ optionalValue (formatDate date) }}</td></tr>
                    <tr><td>Indeterminate</td><td>{{ convertBoolean indeterminate }}</td></tr>
                </table>
            {{/nextReviewDate}}
            {{^nextReviewDate}}
                <p>No Data Held</p>
            {{/nextReviewDate}}

            <p><b>Category decision based on security information</b></p>
            {{#categoryDecisionBasedOnSecurityInformation}}
